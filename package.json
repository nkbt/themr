    "gh-pages": "^1.0.0",
    "glob": "^7.1.2",
    "history": "^4.7.2",
    "html-webpack-include-assets-plugin": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "nightwatch": "^0.9.16",
    "portfinder": "^1.0.13",
    "prop-types": "^15.6.1",
    "qs": "^6.5.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "rimraf": "^2.6.1",
    "serve-static": "^1.12.3",
    "style-loader": "^0.21.0",
    "tape": "^4.8.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.2",
    "webpack-dev-server": "^3.1.4"
  },
  "files": [
    "build",
    "lib",
    "src",
    "LICENSE",
    "package.json",
    "README.md"
  ]
