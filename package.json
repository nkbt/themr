{
  "name": "themr",
  "version": "0.0.0",
  "description": "Theming wrapper for React UI libs",
  "main": "lib/index.js",
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "NODE_ENV=production babel src --out-dir lib --source-maps",
    "prepublish": "npm run build -s",
    "start": "echo Nothing to start yet",
    "test": "echo No tests yet",
    "lint": "eslint --cache .",
    "precommit": "npm run lint -s",
    "prepush": "npm run test -s",
    "postversion": "git push --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nkbt/themr.git"
  },
  "keywords": [
    "component",
    "react-component",
    "react"
  ],
  "author": "Nik Butenko <nik@butenko.me> (http://butenko.me/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nkbt/themr/issues"
  },
  "homepage": "https://github.com/nkbt/themr",
  "peerDependencies": {
    "react": "^0.14 || ^15"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.18.2",
    "babel-eslint": "7.1.0",
    "babel-loader": "6.2.7",
    "babel-plugin-transform-object-rest-spread": "6.16.0",
    "babel-plugin-webpack-loaders": "0.8.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "eslint": "3.9.1",
    "eslint-loader": "1.6.1",
    "eslint-plugin-react": "6.6.0",
    "husky": "0.11.9",
    "react": "15.3.2",
    "rimraf": "2.5.4"
  }
